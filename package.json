    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.6.4",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "source-map-loader": "^4.0.1",
