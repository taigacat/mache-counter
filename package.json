    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint --fix src/**/*.scss",
    "test": "jest --passWithNoTests",
    "test:snapshot": "jest --config jest.config.snap.js --passWithNoTests",
